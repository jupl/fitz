{
  "private": true,
  "scripts": {
    "build": "gatsby build",
    "build:story": "trash story/dist && build-storybook -c story -o story/dist",
    "coverage": "yarn run test --coverage",
    "coverage:watch": "yarn run test:coverage --watch",
    "lint": "yarn run lint:type && yarn run lint:file .",
    "lint:file": "eslint",
    "lint:fix": "yarn run lint:fix:file .",
    "lint:fix:file": "yarn run lint:file --fix",
    "lint:type": "tsc",
    "server": "yarn run server:dev",
    "server:dev": "gatsby develop",
    "server:prod": "yarn run build && gatsby serve",
    "server:story": "start-storybook --ci -c story -p 9001",
    "start": "yarn run lint && yarn run test && yarn run build",
    "test": "jest",
    "test:update": "yarn run test --update-snapshot",
    "test:watch": "yarn run test --watch"
  },
  "dependencies": {
    "@jupl/ts": "^0.9.2",
    "babel-plugin-styled-components": "^1.10.7",
    "cross-env": "^7.0.2",
    "fp-ts": "^2.7.1",
    "gatsby": "^2.24.11",
    "gatsby-image": "^2.4.13",
    "gatsby-plugin-manifest": "^2.4.21",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sharp": "^2.6.22",
    "gatsby-plugin-styled-components": "^3.3.10",
    "gatsby-plugin-ts-config": "^0.2.3",
    "gatsby-source-filesystem": "^2.3.23",
    "gatsby-transformer-sharp": "^2.5.11",
    "io-ts": "^2.2.9",
    "io-ts-reporters": "^1.2.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-is": "^16.13.1",
    "styled-components": "^5.1.1",
    "styled-normalize": "^8.0.7",
    "ts-node": "^8.10.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@jest/globals": "^26.1.0",
    "@storybook/addon-a11y": "^6.0.0-rc.14",
    "@storybook/addon-actions": "^6.0.0-rc.14",
    "@storybook/addon-knobs": "^6.0.0-rc.14",
    "@storybook/addon-storyshots": "^6.0.0-rc.14",
    "@storybook/addon-viewport": "^6.0.0-rc.14",
    "@storybook/react": "^6.0.0-rc.14",
    "@testing-library/react-hooks": "^3.4.1",
    "@types/jest": "^26.0.7",
    "@types/node": "^14.0.26",
    "@types/react": "^16.9.43",
    "@types/react-helmet": "^6.0.0",
    "@types/react-test-renderer": "^16.9.2",
    "@types/styled-components": "^5.1.1",
    "@types/webpack": "^4.41.21",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "babel-preset-react-app": "^9.1.2",
    "eslint": "^7.5.0",
    "jest": "^26.1.0",
    "jest-styled-components": "^7.0.2",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1",
    "trash-cli": "^3.0.0"
  }
}
