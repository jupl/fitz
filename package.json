{
  "private": true,
  "scripts": {
    "build": "gatsby build",
    "lint": "yarn run lint:type && yarn run lint:file .",
    "lint:type": "tsc",
    "lint:file": "eslint --ext .js,.ts,.tsx",
    "lint:fix": "yarn run lint:fix:file .",
    "lint:fix:file": "yarn run lint:file --fix",
    "server": "yarn run server:dev",
    "server:dev": "gatsby develop",
    "server:prod": "yarn run build && gatsby serve",
    "start": "yarn run lint && yarn test && yarn run build",
    "test": "cross-env NODE_OPTIONS=\"-r esm -r tsconfig-paths/register -r ts-node/register/transpile-only -r source-map-support/register\" ava",
    "test:update": "yarn run test --update-snapshots",
    "test:watch": "yarn run test --watch"
  },
  "dependencies": {
    "@jupl/ts": "^0.2.6",
    "babel-plugin-styled-components": "^1.10.7",
    "cross-env": "^7.0.2",
    "fp-ts": "^2.6.6",
    "gatsby": "^2.23.7",
    "gatsby-image": "^2.4.7",
    "gatsby-plugin-manifest": "^2.4.12",
    "gatsby-plugin-react-helmet": "^3.3.4",
    "gatsby-plugin-sharp": "^2.6.12",
    "gatsby-plugin-styled-components": "^3.3.4",
    "gatsby-plugin-ts-config": "^0.2.3",
    "gatsby-source-filesystem": "^2.3.12",
    "gatsby-transformer-sharp": "^2.5.5",
    "io-ts": "^2.2.6",
    "io-ts-reporters": "^1.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^6.1.0",
    "react-is": "^16.13.1",
    "styled-components": "^5.1.1",
    "styled-normalize": "^8.0.6",
    "ts-node": "^8.2.0",
    "tsconfig-paths": "^3.8.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^3.3.0",
    "@types/node": "^14.0.13",
    "@types/react": "^16.8.23",
    "@types/react-helmet": "^6.0.0",
    "@types/react-test-renderer": "^16.9.2",
    "@types/sinon": "^9.0.4",
    "@types/styled-components": "^5.1.0",
    "@types/webpack": "^4.4.35",
    "ava": "^3.9.0",
    "eslint": "6",
    "esm": "^3.2.25",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1",
    "sinon": "^9.0.2",
    "source-map-support": "^0.5.19",
    "testdouble": "^3.16.1"
  }
}
